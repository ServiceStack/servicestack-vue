{
  "name": "vue-tw",
  "version": "0.0.2",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-tw.umd.js",
  "module": "./dist/vue-tw.mjs",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only build-style && run-p build-minify",
    "build-minify": "uglifyjs dist/vue-tw.mjs --compress --mangle -o dist/vue-tw.min.mjs",
    "build-style": "postcss src/tailwind.css -o dist/styles.css",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@servicestack/client": "^1.2.6",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
