{
  "name": "@servicestack/vue",
  "version": "3.0.75",
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/servicestack-vue.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/servicestack-vue.mjs",
    "require": "./dist/servicestack-vue.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only build-style && run-p build-minify",
    "build-minify": "uglifyjs dist/servicestack-vue.mjs --compress --mangle -o dist/servicestack-vue.min.mjs",
    "build-style": "postcss src/tailwind.css -o dist/styles.css",
    "build-copy": "npm run build && npm run copy-src && npm run copy-diffusion && npm run copy-creatorkit",
    "copy-src": "copy dist\\servicestack-vue.min.mjs ..\\ServiceStack\\ServiceStack\\src\\ServiceStack\\js\\servicestack-vue.mjs && copy dist\\servicestack-vue.mjs ..\\ServiceStack\\ServiceStack\\tests\\NorthwindAuto\\wwwroot\\lib\\mjs\\",
    "copy-diffusion": "copy dist\\servicestack-vue.m* ..\\..\\netcore\\VueDiffusion\\MyApp\\wwwroot\\lib\\mjs\\ && copy dist\\index.d.ts ..\\..\\netcore\\VueDiffusion\\MyApp\\wwwroot\\lib\\typings\\@servicestack\\vue\\",
    "copy-creatorkit": "copy dist\\servicestack-vue.m* ..\\..\\netcore\\CreatorKit\\CreatorKit\\wwwroot\\lib\\mjs\\ && copy dist\\servicestack-vue.m* ..\\..\\netcore\\CreatorKit\\CreatorKit\\wwwroot\\js\\",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@servicestack/client": "^2.0.7",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.19",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vue-tsc": "^1.0.24"
  }
}
