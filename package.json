{
  "name": "@servicestack/vue",
  "version": "3.0.37",
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/servicestack-vue.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/servicestack-vue.mjs",
    "require": "./dist/servicestack-vue.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only build-style && run-p build-minify",
    "build-minify": "uglifyjs dist/servicestack-vue.mjs --compress --mangle -o dist/servicestack-vue.min.mjs",
    "build-style": "postcss src/tailwind.css -o dist/styles.css",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@servicestack/client": "^2.0.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.12"
  }
}
